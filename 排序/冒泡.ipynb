   "source": [
    "shuzu=[5,1,4,2,8]\n",
    "\n",
    "n = len(shuzu)\n",
    "for i in range(n-1):\n",
    "    for t in range(n-i-1):\n",
    "        if shuzu[t]>shuzu[t+1]:\n",
    "            temp=shuzu[t+1]\n",
    "            shuzu[t+1]=shuzu[t]\n",
    "            shuzu[t]=temp\n",
    "\n",
    "print(shuzu)"
   ]
  }
